{% load dajaxice_templatetags %}
<!DOCTYPE html>
<html lang="en-ca" class="" dir="ltr">
  <head>
    <title>Django editlive Demo</title>
    <meta charset="utf-8">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='shortcut icon' href="/static/valrive/img/favicon.ico" />
    {# jQuery #}
    <script type="text/javascript" src="{{ STATIC_URL }}website/jquery-ui-1.9.2/js/jquery-1.8.3.min.js"></script>
    {# jQuery UI #}
    <link rel="stylesheet" type="text/css" href="{{ STATIC_URL }}website/jquery-ui-1.9.2/css/smoothness/jquery-ui-1.9.2.custom.min.css">
    <script type="text/javascript" src="{{ STATIC_URL }}website/jquery-ui-1.9.2/js/jquery-ui-1.9.2.custom.min.js"></script>
    {% dajaxice_js_import %}
    {# Bootstrap #}
    <link rel="stylesheet" type="text/css" href="{{ STATIC_URL }}website/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="{{ STATIC_URL }}website/bootstrap/css/bootstrap-responsive.min.css">
    <link rel="stylesheet" type="text/css" href="{{ STATIC_URL }}django_pygments/css/friendly.css" />
    <link rel="stylesheet" type="text/css" href="{{ STATIC_URL }}website/css/styles.css">
    <script type="text/javascript" src="{{ STATIC_URL }}website/bootstrap/js/bootstrap.min.js"></script>
    {# Editlive #}
    <link rel="stylesheet" type="text/css" href="{{ STATIC_URL }}editlive/css/editlive.css">
    <script type="text/javascript" src="{{ STATIC_URL }}editlive/js/jquery.editlive.js"></script>
    <script type="text/javascript" src="{{ STATIC_URL }}editlive/js/jquery.editlive.char.js"></script>
    <script type="text/javascript" src="{{ STATIC_URL }}editlive/js/jquery.editlive.text.js"></script>
    <script type="text/javascript" src="{{ STATIC_URL }}editlive/js/jquery.editlive.date.js"></script>
    <script type="text/javascript" src="{{ STATIC_URL }}editlive/js/jquery.editlive.datetime.js"></script>
    <script type="text/javascript" src="{{ STATIC_URL }}editlive/js/jquery.editlive.boolean.js"></script>
    <script type="text/javascript" src="{{ STATIC_URL }}editlive/js/jquery.editlive.foreignkey.js"></script>
    <script type="text/javascript" src="{{ STATIC_URL }}editlive/js/jquery.editlive.choices.js"></script>
    <script type="text/javascript" src="{{ STATIC_URL }}editlive/js/jquery.editlive.manytomany.js"></script>
    {#<script type="text/javascript" src="{{ STATIC_URL }}editlive/js/jquery.editlive.ajaxform.js"></script>#}
    {#<script type="text/javascript" src="{{ STATIC_URL }}editlive/js/jquery.editlive.listactions.js"></script>#}
  </head>
  <body data-spy="scroll" data-target=".bs-docs-sidebar">
      {% block header %}
      {% include "nav.inc.html" %}
      <header class="jumbotron subhead" id="overview">
          <div class="container">
              <h1>{% block page.title %}Django editlive{% endblock %}</h1>
              <p class="lead">
                {% block page.subtitle %}Live object editing for django{% endblock %}
              </p>
          </div>
      </header>
      {% endblock %}
      {% block content %}{% endblock %}
      {% block footer %}{% endblock %}
      <a href="https://github.com/h3/django-editlive/" id="forkme"><img src="{{ STATIC_URL }}website/forkme.png" alt="Fork me on GitHub"></a>
  </body>
</html>
